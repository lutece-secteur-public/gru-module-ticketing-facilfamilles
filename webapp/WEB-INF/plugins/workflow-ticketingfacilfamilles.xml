<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    <name>workflow-ticketingfacilfamilles</name>
    <class>fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.service.WorkflowTicketingFacilFamillesPlugin</class>
    <version>1.0.4</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>module.workflow.ticketingfacilfamilles.module.description</description>
    <provider>module.workflow.ticketingfacilfamilles.module.provider</provider>
    <provider-url>http://dev.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2016 Mairie de Paris</copyright>
    
    <!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->
    <db-pool-required>1</db-pool-required>
    
       
    <core-version-dependency>
        <min-core-version>5.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>TICKETING_ACTEUR_TERRAIN</feature-id>
            <feature-title>module.workflow.ticketingfacilfamilles.fieldAgentResponse.page.name</feature-title>
            <feature-description>module.workflow.ticketingfacilfamilles.fieldAgentResponse.page.description</feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/ticketing/modules/ticketingfacilfamilles/TicketFieldAgentResponse.jsp</feature-url>
            <feature-icon-url/>
        </admin-feature>
    </admin-features>    
    
    
    <!-- Xpage configuration -->
    <applications>
    </applications>
    
    <!-- Portlet parameters -->
    <portlets>
   	</portlets>
   
    <!-- servlets -->
    <servlets>
        <servlet>
            <servlet-name>FieldAgentSearchServlet</servlet-name>
            <url-pattern>/servlet/plugins/workflow/ticketingfacilfamilles/fieldagentsearch</url-pattern>
            <servlet-class>fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.service.search.FieldAgentSearchServlet</servlet-class>
        </servlet>
        <servlet>
            <servlet-name>FieldAgentEmailValidationServlet</servlet-name>
            <url-pattern>/servlet/plugins/workflow/ticketingfacilfamilles/fieldagentemailvalidation</url-pattern>
            <servlet-class>fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.service.search.FieldAgentEmailValidationServlet</servlet-class>
        </servlet>
    </servlets>
    
    
	<!-- Specific plugin Javascript files -->
    <javascript-files>
    </javascript-files>
    
    <!-- Specific plugin CSS stylesheet -->
	<admin-css-stylesheets>
    </admin-css-stylesheets>
    <css-stylesheets/>
</plug-in>