<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xmlns:tx="http://www.springframework.org/schema/tx" xmlns:jdbc="http://www.springframework.org/schema/jdbc"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd
       http://www.springframework.org/schema/tx
       http://www.springframework.org/schema/tx/spring-tx-3.0.xsd">
       
    <!-- Transaction Manager -->
    <bean id="workflow-ticketingfacilfamilles.transactionManager" class="fr.paris.lutece.portal.service.database.DAOUtilTransactionManager"
        p:pluginName="workflow-ticketingfacilfamilles" />
    
    <!-- DAO -->
    <bean id="workflow-ticketingfacilfamilles.taskTicketFacilFamillesConfigDAO"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.business.config.TaskTicketFacilFamillesConfigDAO" />
    <bean id="workflow-ticketingfacilfamilles.ticketFacilFamillesHistoryDAO"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.business.history.TicketFacilFamillesHistoryDAO" />
        
    <!-- Service -->
    <bean id="workflow-ticketingfacilfamilles.taskTicketFacilFamillesConfigService"
        class="fr.paris.lutece.plugins.workflowcore.service.config.TaskConfigService"
        p:taskConfigDAO-ref="workflow-ticketingfacilfamilles.taskTicketFacilFamillesConfigDAO" />
        
    <!-- NotifyGRU Provider -->
    <bean id="workflow-ticketingfacilfamilles.ProviderService" class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.provider.TicketFacilFamillesNotifyGruProvider" scope="prototype"
        p:key="workflow-ticketingfacilfamilles.ProviderService"
        p:titleI18nKey="module.workflow.ticketingfacilfamilles.task_ticket_facilfamilles.notifygru_provider"
        p:beanName="workflow-ticketingfacilfamilles.ProviderService"   
        p:managerProvider="false" />
        
    <!-- Task config -->
    <bean id="workflow-ticketingfacilfamilles.taskTicketFacilFamillesConfig"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.business.config.TaskTicketFacilFamillesConfig"
        scope="prototype" />    

    <!-- Task Sollicitation Acteur Terrain -->
    <bean id="workflow-ticketingfacilfamilles.taskTicketFacilFamilles"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.service.task.TaskTicketFacilFamilles"
        scope="prototype" />
    <bean id="workflow-ticketingfacilfamilles.taskTypeTicketFacilFamilles"
        class="fr.paris.lutece.plugins.workflowcore.business.task.TaskType"
        p:key="taskTicketingFacilFamilles" p:titleI18nKey="module.workflow.ticketingfacilfamilles.task_ticket_facilfamilles.title"
        p:beanName="workflow-ticketingfacilfamilles.taskTicketFacilFamilles" p:configBeanName="workflow-ticketingfacilfamilles.taskTicketFacilFamillesConfig"
        p:configRequired="true" p:formTaskRequired="true" />
    <bean id="workflow-ticketingfacilfamilles.ticketFacilFamillesTaskComponent"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.web.task.TicketFacilFamillesTaskComponent"
        p:taskType-ref="workflow-ticketingfacilfamilles.taskTypeTicketFacilFamilles"
        p:taskConfigService-ref="workflow-ticketingfacilfamilles.taskTicketFacilFamillesConfigService" />    
    
</beans>