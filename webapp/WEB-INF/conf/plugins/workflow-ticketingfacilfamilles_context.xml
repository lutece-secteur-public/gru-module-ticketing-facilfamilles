<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xmlns:tx="http://www.springframework.org/schema/tx" xmlns:jdbc="http://www.springframework.org/schema/jdbc"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd
       http://www.springframework.org/schema/tx
       http://www.springframework.org/schema/tx/spring-tx-3.0.xsd">
       
    <!-- Site properties -->    
    <bean id="workflow-ticketingfacilfamilles.sitePropertiesGroup" class="fr.paris.lutece.portal.service.site.properties.SitePropertiesGroup" >
        <property name="nameKey"><value>module.workflow.ticketingfacilfamilles.site_properties.name</value></property>
        <property name="descriptionKey"><value>module.workflow.ticketingfacilfamilles.site_properties.description</value></property>
        <property name="keysPrefix"><value>module.workflow.ticketingfacilfamilles.site_property.</value></property>
    </bean>
       
       
    <!-- Transaction Manager -->
    <bean id="workflow-ticketingfacilfamilles.transactionManager" class="fr.paris.lutece.portal.service.database.DAOUtilTransactionManager"
        p:pluginName="workflow-ticketingfacilfamilles" />
    
    <!-- DAO -->
    <bean id="workflow-ticketingfacilfamilles.taskTicketEmailAgentConfigDAO"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.business.config.TaskTicketEmailAgentConfigDAO" />
    <bean id="workflow-ticketingfacilfamilles.ticketEmailAgentHistoryDAO"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.business.history.TicketEmailAgentHistoryDAO" />
    <bean id="workflow-ticketingfacilfamilles.ticketingEmailAgentMessageDAO"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.business.demand.TicketingEmailAgentMessageDAO" />
    <bean id="workflow-ticketingfacilfamilles.fieldAgentUserDAO"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.business.fieldagent.FieldAgentUserDAO" />
        
    <!-- Service -->
    <bean id="workflow-ticketingfacilfamilles.taskTicketEmailAgentConfigService"
        class="fr.paris.lutece.plugins.workflowcore.service.config.TaskConfigService"
        p:taskConfigDAO-ref="workflow-ticketingfacilfamilles.taskTicketEmailAgentConfigDAO" />
        
    <!-- NotifyGRU Provider -->
    <bean id="workflow-ticketingfacilfamilles.EmailAgentProviderService" class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.provider.TicketEmailAgentNotifyGruProvider" scope="prototype"
        p:key="workflow-ticketingfacilfamilles.EmailAgentProviderService"
        p:titleI18nKey="module.workflow.ticketingfacilfamilles.task_ticket_emailagent.notifygru_provider"
        p:beanName="workflow-ticketingfacilfamilles.EmailAgentProviderService"   
        p:managerProvider="false" />
        
    <!-- Task config -->
    <bean id="workflow-ticketingfacilfamilles.taskTicketEmailAgentConfig"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.business.config.TaskTicketEmailAgentConfig"
        scope="prototype" />    

    <!-- Task Sollicitation Acteur Terrain -->
    <bean id="workflow-ticketingfacilfamilles.taskTicketEmailAgent"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.service.task.TaskTicketEmailAgent"
        scope="prototype" />
    <bean id="workflow-ticketingfacilfamilles.taskTypeTicketEmailAgent"
        class="fr.paris.lutece.plugins.workflowcore.business.task.TaskType"
        p:key="taskTicketingEmailAgent" p:titleI18nKey="module.workflow.ticketingfacilfamilles.task_ticket_emailagent.title"
        p:beanName="workflow-ticketingfacilfamilles.taskTicketEmailAgent" p:configBeanName="workflow-ticketingfacilfamilles.taskTicketEmailAgentConfig"
        p:configRequired="true" p:formTaskRequired="true" />
    <bean id="workflow-ticketingfacilfamilles.ticketEmailAgentTaskComponent"
        class="fr.paris.lutece.plugins.workflow.modules.ticketingfacilfamilles.web.task.TicketEmailAgentTaskComponent"
        p:taskType-ref="workflow-ticketingfacilfamilles.taskTypeTicketEmailAgent"
        p:taskConfigService-ref="workflow-ticketingfacilfamilles.taskTicketEmailAgentConfigService" />    
    
   <!-- SignRequest -->
   <bean id="workflow-ticketingfacilfamilles.hashService"
      class="fr.paris.lutece.util.signrequest.security.Sha1HashService" />
   <bean id="workflow-ticketingfacilfamilles.requestAuthentication"
      class="fr.paris.lutece.util.signrequest.RequestHashAuthenticator">
      <property name="hashService" ref="workflow-ticketingfacilfamilles.hashService" />
      <property name="signatureElements">
         <list>
            <value>id_message_agent</value>
         </list>
      </property>
      <property name="privateKey">
         <value>wkFFemailAgent</value>
      </property>
   </bean>
</beans>